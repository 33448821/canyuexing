<!--pages/index/index.wxml-->
<view style="position:absolute;top:0px;bottom:0px;left:0px;right:0px;">
  <image onload="unLoading" src="../../image/back.png" style="height:100%;width:100%;" />
</view>

<view style="position:absolute;bottom:10px;left:10px;right:10px;height:130px;border-radius:10px;background:rgba(217, 236, 255,0.7);">
  <view style="float:left;height:100%;width:50%;position:relative;">
    <navigator url='../canteen/canteen' open-type="switchTab">
      <view
        style="position:absolute;top:10px;left:10px;right:5px;bottom:10px;background:#E6A23C;border-radius:10px;text-align:center;font-size:20px;font-weight:600;color:#606266;">
        <view style="width:100%;height:55px;line-height:90px;">
          <image src="../../image/food.png" style="height:30px;width:30px;" />
        </view>
        <view style="width:100%;height:55px;line-height:35px;">
          开始点餐
        </view>
      </view>
    </navigator>
  </view>
  <view style="float:left;height:100%;width:50%;position:relative;">
    <navigator url='../details/details' open-type="switchTab">
      <view
        style="position:absolute;top:10px;left:5px;right:10px;bottom:10px;background:#E6A23C;border-radius:10px;text-align:center;font-size:20px;font-weight:600;color:#606266;">
        <view style="width:100%;height:55px;line-height:90px;">
          <image src="../../image/icon_component.png" style="height:28px;width:28px;" />
        </view>
        <view style="width:100%;height:55px;line-height:35px;">
          个人中心
        </view>
      </view>
    </navigator>
  </view>
</view>